digraph ArticleVsRental {
  graph [
    rankdir=LR,
    bgcolor=white,
    fontname="Helvetica",
    fontsize=12,
    labelloc="t",
    label="Article vs Rental: The Fundamental Difference"
  ];

  node [
    shape=box,
    style="rounded,filled",
    fontname="Helvetica",
    fontsize=11,
    margin="0.3,0.2"
  ];

  edge [
    color="#4B5563",
    penwidth=2,
    arrowsize=0.9,
    fontname="Helvetica",
    fontsize=10
  ];

  subgraph cluster_article {
    label="Article Subscription";
    color="#16A34A";
    style="rounded";
    bgcolor="#F0FDF4";

    Article [label="Article A\n(content)", fillcolor="#BBF7D0", color="#16A34A"];

    User1 [label="User 1", fillcolor="#E5E7EB", color="#9CA3AF", shape=ellipse];
    User2 [label="User 2", fillcolor="#E5E7EB", color="#9CA3AF", shape=ellipse];
    User3 [label="User 3", fillcolor="#E5E7EB", color="#9CA3AF", shape=ellipse];

    User1 -> Article [color="#16A34A"];
    User2 -> Article [color="#16A34A"];
    User3 -> Article [color="#16A34A"];

    NoConflict [label="(no conflict)", fillcolor="#DCFCE7", color="#16A34A", shape=plaintext, fontsize=10, fontstyle=italic];
  }

  subgraph cluster_rental {
    label="Rental (Exclusive)";
    color="#F59E0B";
    style="rounded";
    bgcolor="#FFFBEB";

    Rental [label="Venue / Car\n(asset)", fillcolor="#FEF3C7", color="#F59E0B"];

    UserA [label="User A\n(now)", fillcolor="#FDE68A", color="#D97706", shape=ellipse];
    UserB [label="User B\n(blocked)", fillcolor="#FEE2E2", color="#DC2626", shape=ellipse];

    UserA -> Rental [color="#16A34A", penwidth=2.5];
    UserB -> Rental [color="#DC2626", style=dashed, penwidth=2, label="blocked"];

    Exclusive [label="(exclusive access)", fillcolor="#FEF3C7", color="#F59E0B", shape=plaintext, fontsize=10, fontstyle=italic];
  }
}
